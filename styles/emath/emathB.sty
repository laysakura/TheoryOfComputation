% 小数を丸める
%   \Scut[#1]#2#3#4
%     小数 #2 を小数点以下 #3 位までとする．結果を #4 に返す．
%     #1 : r (default) 四捨五入
%     #1 : t (trunc)   切り捨て
%
\def\Scut{\@ifnextchar[{\@Scut}{\@Scut[r]}}
\def\@Scut[#1]#2#3#4{%
  \if r#1\FPround#4{#2}{#3}\else
  \if t#1\FPtrunc#4{#2}{#3}\fi\fi
}
