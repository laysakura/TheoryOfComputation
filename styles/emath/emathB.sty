% F 型分数の符号判定
%
% \Fhugou#1#2
%   #1 : F型分数
%   #2 : 結果を受け取るコントロールシーケンス
%        （戻り値） #1>0 のときは 1
%                   #1=0 のときは 0
%                   #1<0 のときは -1
%
\def\Fhugou#1#2{\FtoB{#1}\Fhugou@a\Fhugou@b
  \IMul\Fhugou@a\Fhugou@b\Fhugou@a
  \ifnum\Fhugou@a>\z@\edef#2{1}\else
  \ifnum\Fhugou@a<\z@\edef#2{-1}\else
  \edef#2{0}\fi\fi
}%
\def\Fsign#1#2{\FtoB{#1}\Fhugou@a\Fhugou@b
  \ifnum\Fhugou@a=\z@\edef#2{0}\relax
  \else\ifnum\Fhugou@a>\z@
    \ifnum\Fhugou@b<\z@\edef#2{-}\relax
    \else\edef#2{+}\relax
    \fi
  \else
    \ifnum\Fhugou@b>\z@\edef#2{-}\relax
    \else\edef#2{+}\relax
    \fi
  \fi\fi
}%
% F 型分数の大小比較
