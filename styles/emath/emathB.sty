\def\FPower#1#2#3{%
  \ifnum #2=\z@\edef\Fpower@v{1}%
  \else\ifnum #2>\z@
    \Cfor{\edef\Fpower@i{#2}\edef\Fpower@v{1}}{\Fpower@i>0}{%
      \Decr\Fpower@i}\do{\FMul\Fpower@v{#1}\Fpower@v}%
  \else
