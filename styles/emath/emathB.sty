%        （戻り値） #1>#2 のときは 1
%                   #1=#2 のときは 0
%                   #1<#2 のときは -1
%
\def\Fhikaku#1#2#3{\FSub{#1}{#2}\Fhikaku@s\Fhugou\Fhikaku@s #3}

% ---------------------------------------------- Fcalc ----------
\def\Fcalc#1=#2{%
  \headchar{#1}\FC@h\FC@r
  \if -\FC@h
    \edef\FC@a{(0)#1}
  \else
    \edef\FC@a{#1}%
  \fi
  \@Fcalc\FC@a
  \edef#2{\@koui}}%
\def\@Fcalc#1{%
  \@parse{#1}{@kou}{@opcode}{@lvl}\nlvl
  \Cfor{\edef\max@klvl{0}\edef\@i{0}}{\@i<\nlvl}{}\do{%
    \Incr\@i
    \ifnum\csname @lvl\romannumeral\@i\endcsname>\max@klvl
