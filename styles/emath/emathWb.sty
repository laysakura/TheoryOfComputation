    \edef\zyo@beki{\zyo@ca}\xDecr\zyo@beki
    \@hidari&\@Fseikei{zyo@a}{\zyo@ca}\\
    \xdef\zyo@ca{\zyo@cb}
    \edef\zyo@beki{\zyo@cb}\xDecr\zyo@beki
    -\Big)&\@Fseikei{zyo@b}{\zyo@cb}\\[.5ex]\hline
    \edef\zyo@ca{\zyo@cb}%
    \edef\zyo@beki{\zyo@cq}\xDecr\zyo@beki
    &\@Fseikei{zyo@q}{\zyo@cq}
