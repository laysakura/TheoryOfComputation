\def\Unitvec#1#2{%
  \Absvec{#1}\uv@a
  \ifdim\uv@a\p@=\z@\relax
    \edef#2{#1}%
  \else
    \vecXY{#1}\uv@x\uv@y
    \Div\uv@x\uv@a\uv@x
    \Div\uv@y\uv@a\uv@y
    \edef#2{(\uv@x,\uv@y)}%
  \fi
}%
\def\Absvec#1#2{%
  \vecXY{#1}\Absvec@x\Absvec@y
  \Mul\Absvec@x\@xscale\Absvec@x
  \Mul\Absvec@y\@yscale\Absvec@y
  \PhAbsvec{(\Absvec@x,\Absvec@y)}#2}%
