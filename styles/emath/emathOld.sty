    }%
\def\@@@@@@edaMon[#1](#2)"#3""#4""#5""#6""#7"{%
    \pre@edaMon
    \ifdim#2>0pt%
        \templa=#2%
    \else
        \templa=\linewidth%
        \addtolength{\templa}{-\columnsep}%
        \divide\templa by #1%
    \fi%
    \def\tmp{#3}%
    \ifx\tmp\ow@ri@s\else\edaMon@sub%
    \def\tmp{#4}%
    \ifx\tmp\ow@ri@s\else\edaMon@sub%
    \def\tmp{#5}%
    \ifx\tmp\ow@ri@s\else\edaMon@sub%
    \def\tmp{#6}%
    \ifx\tmp\ow@ri@s\else\edaMon@sub%
    \def\tmp{#7}%
    \ifx\tmp\ow@ri@s\else\edaMon@sub%
    \fi\fi\fi\fi\fi%
    }%
\def\edaMon@sub{%
        \begin{minipage}[t]{\templa}\stepcounter{edaban}\edaMonLabel@s%
        \tmp\end{minipage}%
    }%
