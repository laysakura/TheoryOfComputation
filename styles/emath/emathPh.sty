%
% 折れ線で囲まれた図形の面積
%
\def\MensekiP#1#2{%
  \CalcVal{%
    $tmpa="#1";
%
    $i=index($tmpa,",");
    $xo=substr($tmpa,1,$i-1);
    $tmpa=substr($tmpa,$i+1);
    $i=index($tmpa,")");
    $yo=substr($tmpa,0,$i);
    $tmpa=substr($tmpa,$i+1);
%
    $i=index($tmpa,",");
    $xi=substr($tmpa,1,$i-1);
    $tmpa=substr($tmpa,$i+1);
    $i=index($tmpa,")");
    $yi=substr($tmpa,0,$i);
    $tmpa=substr($tmpa,$i+1);
    $xi-=$xo;
    $yi-=$yo;
%
    $menseki=0;
    for ($i=index($tmpa,",");$i>=0;){
      $xii=substr($tmpa,1,$i-1);
      $tmpa=substr($tmpa,$i+1);
      $i=index($tmpa,")");
      $yii=substr($tmpa,0,$i);
      $xii-=$xo;
      $yii-=$yo;
      $menseki+=($xi*$yii-$xii*$yi)/2;
      $tmpa=substr($tmpa,$i+1);
      $xi=$xii;
      $yi=$yii;
      $i=index($tmpa,",");
    }
    printf FHNDL "\EMpercentchar s\string\n",$menseki;
  }#2
}
