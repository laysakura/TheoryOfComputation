%%                s : 斜線（／） [デフォルト]
%%                r : 斜線（＼）
%%                h : 横線
%%                d : 横二本線
%%            #2: 線の色
%%            #3: 線を引く対象
%%            #4: 訂正後の文字位置
%%                   r = 右上 (=tr=rt)（デフォルト）
%%                   l = 左上 (=tl=lt)
%%                   t = 上
%%                   b = 下
%%                   rb=br=右下
%%                   lb=bl=左下
%%            #5: 訂正後の文字列 (scriptstyle)
%%            #6: 斜線の位置を修正するベクトル
%%
\def\Teisei{%
    \@ifnextchar[{\@Teisei}{\@Teisei[s]}}%
\def\@Teisei[#1]{%
