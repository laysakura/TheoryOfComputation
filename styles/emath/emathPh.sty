  \vecXY\EandL@tmp\EandL@x\EandL@y\Mul\EandL@y\EandL@hi\EandL@y
  \edef#5{(\EandL@x,\EandL@y)}\relax
}%
\def\DaenniSessen#1#2#3#4#5#6{%
