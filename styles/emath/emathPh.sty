\def\Tyokkakukigou{\begingroup\ifnum\EMps@mode=\@ne\gsave\fi
%
  \define@key{emP}{kakukipos}{\def\kakuki@pos{##1}}%
  \define@key{emP}{kakukikaiten}{\ifdim ##1\p@<\z@\IAdd{##1}{360}\kakuki@pos
    \else\edef\kakuki@pos{##1}\fi}%
  \define@key{emP}{kakukisize}{\def\kakuki@size{##1}}%
  \define@key{emP}{size}{\def\size@{##1}}%
  \define@key{emP}{kakukinoudo}{\def\kakuki@noudo{##1}}%
%
  \edef\kakuki@pos{0}%
  \edef\size@{5pt}%
  \edef\kakuki@noudo{0}%
  \@ifnextchar<{\Tyokkaku@kigou}{\@Tyokkakukigou}}%
\def\Tyokkaku@kigou<#1>{\ifx\empty #1\else\setkeys{emP}{#1}\fi\@Tyokkakukigou}%
\def\@Tyokkakukigou{%
  \@tempdima\size@
  \edef\size@@{\strip@pt\@tempdima}%
  \Add\kakuki@pos\kakuki@noudo\kakuki@pos@noudo
  \@ifnextchar[{\@@Tyokkakukigou}{\@@Tyokkakukigou[\kakuki@pos@noudo]}}%
\def\@@Tyokkakukigou[#1]{\@ifnextchar({\@@@Tyokkakukigou[#1]}{%
  \@@@Tyokkakukigou[#1](\size@@)}}%
\def\@@@Tyokkakukigou[#1](#2)#3#4#5{%
\Kyorii{#3}{#4}\a@Tyokkakukigou
\Kyorii{#5}{#4}\b@Tyokkakukigou
\ifdim\a@Tyokkakukigou pt>0.005pt\relax
\ifdim\b@Tyokkakukigou pt>0.005pt\relax
  \Div{#2}{\strip@pt\unitlength}\u@pt%
  \Subvec{#3}{#4}\t@x\Unitvec\t@x\t@x\Mulvec\u@pt\t@x\t@x
  \Subvec{#5}{#4}\t@y\Unitvec\t@y\t@y\Mulvec\u@pt\t@y\t@y
  \Addvec\t@x\t@y\t@z\Addvec{#4}\t@x\t@x%
  \Addvec{#4}\t@y\t@y \Addvec{#4}\t@z\t@z%
  \ifdim #1\p@>\z@
    \ifdim #1\p@<90\p@
      \edef\Tykk@kaiten{0}%
      \edef\Tykk@noudo{#1}%
    \else\ifdim #1\p@<180\p@
      \edef\Tykk@kaiten{1}%
      \Sub{#1}{90}\Tykk@noudo
    \else\ifdim #1\p@<270\p@
      \edef\Tykk@kaiten{2}%
      \Sub{#1}{180}\Tykk@noudo
    \else
      \edef\Tykk@kaiten{3}%
      \Sub{#1}{270}\Tykk@noudo
    \fi\fi\fi
    \ifcase\Tykk@kaiten
        \ifdim\Tykk@noudo\p@>\z@
          \Nuritubusi[\Tykk@noudo]{\t@x\t@z\t@y#4\t@x}%
        \fi
        \Drawline{\t@x\t@z\t@y}%
      \or
        \@Bunten{#3}{#4}{2}{-1}\t@xx
        \Tyokkakukigou[\Tykk@noudo](#2){#5}{#4}{\t@xx}%
      \or
        \@Bunten{#3}{#4}{2}{-1}\t@xx
        \@Bunten{#5}{#4}{2}{-1}\t@yy
        \Tyokkakukigou[\Tykk@noudo](#2){\t@xx}{#4}{\t@yy}%
      \or
        \@Bunten{#5}{#4}{2}{-1}\t@yy
        \Tyokkakukigou[\Tykk@noudo](#2){\t@yy}{#4}{#3}%
    \fi
  \else
    \Drawline{\t@x\t@z\t@y}%
  \fi
\fi\fi
\ifnum\EMps@mode=\@ne\grestore\fi
\endgroup
}%
\let\perpmark\Tyokkakukigou
