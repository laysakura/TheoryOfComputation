  \fi
\fi
  \For\@t\kaku@s\kaku@e{\YG@unit}\Do{\Cos\@t\@x\Mul\@x{\@hankei}\@x%
    \Sin\@t\@y\Mul\@y{\@hankei}\@y
    \Addvec{(\@x,\@y)}\@tyuusin\tmp@P
    \edef\@resen{\@resen\tmp@P}}%
  \Cos\kaku@e\@x\Mul\@x{\@hankei}\@x\Sin\kaku@e\@y\Mul\@y{\@hankei}\@y%
  \Addvec{(\@x,\@y)}\@tyuusin\tmp@P
