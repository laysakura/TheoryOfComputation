    \ensuremath{%
      \stackrel{\box1}%
        {\box\EM@boxa\relax{%
          \begin{picture}(0,0)%
            \put(-\teisei@w,0){%
              \ifx\empty#2\else\color{#2}\fi%
              \ifthenelse{\equal{#5}\empty}{}{\setkeys{emP}{#5}}%
              \if#1r\relax
                \Syasen{\LT\RB}%
              \else\if#1h\relax
                \Syasen{\LT\RT}%
              \else\if#1d\relax
                \Syasen{\LT\RT}%
                \Syasen{\LB\RB}%
              \else\if#1x\relax
                \Syasen{\LB\RT}%
                \Syasen{\LT\RB}%
              \else\if#1S\relax
                \put(0,1){\Syasen{\LB\RT}}%
                \put(0,-1){\Syasen{\LB\RT}}%
              \else
                \Syasen{\LB\RT}%
              \fi\fi\fi\fi\fi
            }%
          \end{picture}%
        }}%
    }%
   \fi%
}}%
