  \Cfor{\edef\DW@i{1}}{\DW@i<\TH@N}{\edef\DW@S{\DW@E}}\do{%
      \Incr\DW@i
      \edef\DW@E{\hairetu{TH@V}{\DW@i}}%
      \@EMpsDrawhwave@sub\DW@S\DW@E
  }%
  \endgroup
}%
