\def\prassociates#1{%
  \prComplex{#1},\ \relax
  \GMul{#1}{-1,0}\prdouhan@tmp\prComplex\prdouhan@tmp,\ \relax
  \GMul{#1}{0,1}\prdouhan@tmp\prComplex\prdouhan@tmp,\ \relax
  \GMul{#1}{0,-1}\prdouhan@tmp\prComplex\prdouhan@tmp.
}%
