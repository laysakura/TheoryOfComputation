\def\GDiv#1#2#3{%
  \GNorm{#2}\GDiv@bunbo
  \GConj{#2}\GDiv@tmp
  \GMul\GDiv@tmp{#1}\GDiv@tmp
  \RIbunkai{\GDiv@tmp}\GDiv@r\GDiv@i
  \edef#3{\GDiv@r/\GDiv@bunbo,\GDiv@i/\GDiv@bunbo}%
}%
