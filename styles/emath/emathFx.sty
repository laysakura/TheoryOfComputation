  \def\xybekutoru@t##1{{%
    \setbox0=\hbox{\ensuremath{\m@th\displaystyle\text{##1}\bekutoru@sityuu}}%
    \ifdim\wd0<1em\relax
      \beku{\ensuremath{\m@th\displaystyle\text{##1}\bekutoru@sityuu}}%
